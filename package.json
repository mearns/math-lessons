{
  "name": "math-lessons",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/",
  "private": true,
  "scripts": {
    "clean": "rimraf coverage/ .nyc_output/ out/ dist/",
    "check:lint:readme:markdown": "remark --no-stdout README.md",
    "check:lint:readme": "npm-run-all --parallel check:lint:readme:*",
    "check:lint:js:root": "standard ./*.js | snazzy",
    "check:lint:js:src": "standard src/**/*.js | snazzy",
    "check:lint:js": "npm-run-all --parallel check:lint:js:*",
    "check:lint": "npm-run-all --parallel check:lint:*",
    "check": "npm-run-all --parallel check:*",
    "verify": "npm-run-all check",
    "compile:babel": "babel src -d dist/src/",
    "compile:webpack": "webpack --config ./webpack.config.js",
    "compile": "npm-run-all compile:*",
    "build": "npm-run-all clean verify compile",
    "start": "node .",
    "watch": "webpack --watch"
  },
  "author": "Brian Mearns <mearns.b@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.2.0",
    "@babel/register": "7.0.0",
    "babel-loader": "8.0.4",
    "cross-env": "5.2.0",
    "if-env": "1.0.4",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.5",
    "remark-cli": "6.0.1",
    "rimraf": "2.6.2",
    "snazzy": "8.0.0",
    "standard": "12.0.1",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "bluebird": "3.5.3",
    "express": "4.18.2",
    "extrinsic-promises": "2.1.0",
    "props-model": "0.2.0",
    "tinycolor2": "1.4.1"
  }
}
